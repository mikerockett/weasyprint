<?xml version="1.0" encoding="UTF-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/10.4/phpunit.xsd" beStrictAboutOutputDuringTests="true" bootstrap="vendor/autoload.php" colors="true" cacheDirectory=".phpunit.cache">
	<coverage />
	<testsuites>
		<testsuite name="Core">
			<directory suffix="Test.php">./tests/Core</directory>
		</testsuite>
		<testsuite name="Laravel">
			<directory suffix="Test.php">./tests/Laravel</directory>
		</testsuite>
		<testsuite name="All">
			<directory suffix="Test.php">./tests/Core</directory>
			<directory suffix="Test.php">./tests/Laravel</directory>
		</testsuite>
	</testsuites>
	<source>
		<include>
			<directory suffix=".php">
				./src
			</directory>
		</include>
	</source>
</phpunit>
