{
  "name": "rockett/weasyprint",
  "description": "A feature-rich Laravel wrapper for the WeasyPrint Document Factory.",
  "type": "library",
  "license": "ISC",
  "authors": [
    {
      "name": "Mike Rockett",
      "email": "os@rockett.pw"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": "^8.2",
    "composer/semver": "^3.4",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "rockett/pipeline": "^3.0|^4.0",
    "symfony/process": "^6.0|^7.0"
  },
  "require-dev": {
    "johnbacon/stout": "^1.13",
    "nunomaduro/collision": "^8.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^3.0",
    "smalot/pdfparser": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "WeasyPrint\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeasyPrint\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "@dump",
      "@pest"
    ],
    "dump": "composer dumpautoload",
    "pest": "./vendor/bin/pest --profile",
    "format": "./vendor/bin/stout --dirty",
    "format:all": "./vendor/bin/stout"
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeasyPrint\\Provider"
      ]
    }
  }
}
